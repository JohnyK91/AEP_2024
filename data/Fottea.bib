@article{ WOS:000373585200003,
Author = {Javornicky, Pavel},
Title = {The occurrence of microscopic algae with blue-green chloroplasts or with
   endocyanelles (Glaucophyta) in the fresh waters of the C<i>z</i>ech
   Republic, with a new report of <i>Cryptella</i> <i>cyanophora</i>
   PASCHER},
Journal = {FOTTEA},
Year = {2016},
Volume = {16},
Number = {1},
Pages = {22-33},
Abstract = {One of the observed flagellates, Cryptella cyanophora PASCHER, is
   probably the first discovery ever of this species with cyanelles after
   more than 80 years since its description. This species was previously
   classified into the cryptophycean genus Chroomonas HANSGIRG (see
