@article{ WOS:000291511200002,
Author = {Oren, Aharon},
Title = {Naming Cyanophyta/Cyanobacteria - a bacteriologist's view},
Journal = {FOTTEA},
Year = {2011},
Volume = {11},
Number = {1},
Pages = {9-16},
Month = {JUL},
Note = {18th Symposium of the
   International-Association-for-Cyanobacteria/Cyanophyte Research, Ceske
   Budejovice, CZECH REPUBLIC, AUG 15-20, 2010},
Organization = {Int Assoc Cyanophyte Res (IAC)},
Abstract = {This paper provides information on the origin and the etymology of the
   names of genera of Cyanophyta/Cyanobacteria in current use and on the
   way new names are formed under the provisions of the International Code
   of Botanical Nomenclature and the International Code of Nomenclature of
   Prokaryotes. Special emphasis is placed on the differences between the
   two Codes that should be taken into account when creating new names
