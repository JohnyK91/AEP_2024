@article{ WOS:000291511200022,
Author = {Leon-Tejera, Hilda and Perez-Estrada, Claudia J. and Montejano, Gustavo
   and Serviere-Zaragoza, Elisa},
Title = {Biodiversity and temporal distribution of Chroococcales
   (Cyanoprokaryota) of an arid mangrove on the east coast of Baja
   California Sur, Mexico},
Journal = {FOTTEA},
Year = {2011},
Volume = {11},
Number = {1},
