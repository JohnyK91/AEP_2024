@article{ WOS:000276324000004,
Author = {Wujek, Daniel E. and Kadiri, Medina O. and Dziedzic, Ryan M.},
Title = {Silica-scaled Chrysophyceae and Synurophyceae from Nigeria. III.
   Chrysophytes from rivers of Edo State},
Journal = {FOTTEA},
Year = {2010},
Volume = {10},
Number = {1},
Pages = {93-98},
Month = {JUL},
Abstract = {A total of 18 species of silica-scaled chrysophytes (Chrysophyceae: one
   species each in Chrysosphaerella, Spiniferomonas, and Paraphysomonas;
   Synurophyceae: 11 Mallomonas spp. and four Synura spp.) was recorded
   from river samples collected in Edo State, Benin, Nigeria, from both dry
   and rainy seasons in 2003. Identifications were based on transmission
   electron microscopy. Five are new records for Nigeria, including lour
   being newly reported for Africa. Two further species could not be
   identified.},
