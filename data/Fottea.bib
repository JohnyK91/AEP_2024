@article{ WOS:000291511200003,
Author = {Engene, Niclas and Gerwick, William H.},
Title = {Intra-genomic 16S rRNA gene heterogeneity in cyanobacterial genomes},
Journal = {FOTTEA},
Year = {2011},
Volume = {11},
Number = {1},
Pages = {17-24},
