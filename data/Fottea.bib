@article{ WOS:000302453000001,
Author = {Prochazkova, Lenka and Houk, Vaclav and Nedbalova, Linda},
Title = {<i>Discostella tatrica</i> sp nov (Bacillariophyceae) - a small centric
   diatom from the Tatra Mountain lakes (Slovakia/Poland)},
Journal = {FOTTEA},
