@article{ WOS:000291511200021,
Author = {Ferrari, Graciela and del Carmen Perez, Maria and Dabezies, Maria and
   Miguez, Diana and Saizar, Carlos},
Title = {Planktic Cyanobacteria in the Lower Uruguay River, South America},
