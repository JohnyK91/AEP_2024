@article{ WOS:000291511200017,
Author = {Kastovsky, Jan and Fucikova, Karolina and Hauer, Tomas and Bohunicka,
   Marketa},
Title = {Microvegetation on the top of Mt. Roraima, Venezuela},
Journal = {FOTTEA},
Year = {2011},
Volume = {11},
Number = {1},
Pages = {171-186},
