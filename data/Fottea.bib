@article{ WOS:000334587400003,
Author = {Taylor, Jonathan C. and Cocquyt, Christine and Karthick, Balasubramanian
   and Van de Vijver, Bart},
Title = {Analysis of the type of <i>Achnanthes exigua</i> GRUNOW
   (Bacillariophyta) with the description of a new Antarctic diatom species},
Journal = {FOTTEA},
Year = {2014},
Volume = {14},
Number = {1},
Pages = {43-51},
Abstract = {A study was made of the type material of Achnanthidium exiguum (GRUNOW)
   CZARNECKI. This often encountered and apparently ubiquitous species was
   originally described as Stauroneis exilis by KUTZING from the island of
   Trinidad and later re-named as Achnanthes exigua by GRUNOW. The type
   material for this taxon (KUTZING sample 397), housed in the VAN HEURCK
   collection at the National Botanic Garden of Belgium, Meise was
   investigated and the ultrastructure of this taxon was documented using
   both LM and SEM observations. A new Antarctic taxon, formerly identified
   as A. exigua, was described as a new species, Achnanthidium
   australexiguum sp. nov. The position of both taxa within the genus
   Achnanthidium is briefly discussed.},
Publisher = {CZECH PHYCOLOGICAL SOC},
Address = {BENATSKA 2,, PRAHA 2, CZ-128 01, CZECH REPUBLIC},
Type = {Article},
Language = {English},
