@article{ WOS:000296310200002,
Author = {Krienitz, Lothar and Bock, Christina},
Title = {<i>Elongatocystis ecballocystiformis</i> gen. et comb. nov., and some
   reflections on systematics of Oocystaceae (Trebouxiophyceae,
   Chlorophyta)},
Journal = {FOTTEA},
Year = {2011},
Volume = {11},
Number = {2},
Pages = {271-278},
Month = {NOV},
Abstract = {Three new strains of members of the family Oocystaceae collected in
   inland waters of Africa were studied microscopically and by molecular
   phylogeny. The new genus Elongatocystis was decribed, and the new
   combination Elongatocystis ecballocystiformis was proposed. The
   phylogenetic position of Oocystidium sp., and Quadricoccus ellipticus
   within the family was shown. The SSU rRNA phylogeny of Oocystaceae
   recovered a need for further studies to display the generic and species
   concept in this monophyletic group of green algae. The essential
   research steps were discussed.},
