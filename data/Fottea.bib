@article{ WOS:000879580700004,
Author = {Berthold, David E. and Werner, Vera R. and Lefler, Forrest W. and Simon,
   Isabella P. and Laughinghouse, I. V. H. Dail},
Title = {The novel marine cyanobacterium <i>Nunduva sanctimaloensis</i> sp. nov.
   (Nostocales, Cyanobacteria) from rocky shores and its reproduction
   through modified monocytes},
Journal = {FOTTEA},
Year = {2022},
Volume = {22},
Number = {2},
Pages = {192-203},
Abstract = {Rocky intertidal regions are diverse as they provide many unique
   habitats for the growth of cyano-bacteria. From outcrops battered by
   waves, tidal pools, and periods of desiccations due to tides, these
   regions offer distinct environments and much of their cyanobacterial
   diversity remains unexplored. To help elucidate cyanobacteria from this
   habitat, one nostocalean cyanobacterium from Brittany, France was
   isolated and cultured. Typical morphological features including
   filaments with heteropolar trichomes, basal or intercalary heterocytes,
   false branching, and long hyaline apical hairs suggested the genus
   Nunduva. The combination of 16S rRNA gene and 16S-23S rRNA internal
   transcribed spacer (ITS), along with diagnosable morphological
   characteristics supports a distinct taxon from previously described
   species. Herein, the novel false branching cyanobacterium Nunduva
   sanctimaloensis sp. nov. is described along with its reproduction
   through modified monocytes.},
