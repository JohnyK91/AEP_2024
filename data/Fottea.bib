@article{ WOS:000581128100003,
Author = {Li, Yanling and Liao, Mengna and Metzeltin, Ditmar},
Title = {Three new <i>Navicula</i> (Bacillariophyta) species from an
   oligotrophic, deep lake, China},
Journal = {FOTTEA},
Year = {2020},
Volume = {20},
Number = {2},
Pages = {121-127},
Abstract = {Three new freshwater diatoms are described from China's Yunnan Plateau
   region in sediments from Lake Fuxian. Navicula australasiatica nov. sp.,
   Navicula perangustissima nov. spec., and Navicula turriformis nov. spec.
   were identified from light and scanning electron micrographs. All new
   species are sufficiently different from other similar species based on
   valve outline, shape of the axial and center areas, and stria density.
   These three new species are all observed in surficial sediment from Lake
   Fuxian and compared to morphologically similar species from the genus
   Navicula.},
Publisher = {CZECH PHYCOLOGICAL SOC},
Address = {BENATSKA 2,, PRAHA 2, CZ-128 01, CZECH REPUBLIC},
