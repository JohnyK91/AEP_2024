   Neustupa, Jiri},
Title = {Analysis of the type material of <i>Pinnularia divergentissima</i>
   (GRUNOW in VAN HEURCK) CLEVE (Bacillariophyceae)},
Journal = {FOTTEA},
Year = {2013},
Volume = {13},
