   Princiotta, Sarah Devaul, Penn State Univ Schuylkill, Dept Biol, Schuylkill Haven, PA 17972 USA.
   Nguyen, Tiffany; Sanders, Robert W., Temple Univ, Dept Biol, Philadelphia, PA 19122 USA.},
DOI = {10.5507/fot.2022.020},
ISSN = {1802-5439},
Keywords = {bacterivory; Dinobryon; flagellate; mixotrophy; phagotrophy; seasonal
   succession},
